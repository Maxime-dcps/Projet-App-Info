@model Projet_mvc.Models.Listing.ListingFormViewModel


<h2>Modifier cette annonce</h2>

<form asp-action="Edit" method="post" enctype="multipart/form-data">

    <partial name="_ListingForm" model="Model" />

    <div class="mt-4">
        <button type="submit" class="btn btn-success">Modifier l'annonce</button>
        <a asp-controller="Home" asp-action="Index" class="btn btn-secondary">Annuler</a>
    </div>
</form>

@section Scripts {
    @{
        await Html.RenderPartialAsync("_ValidationScriptsPartial");
    }
}